{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/distribute-profits",
      "schedule": "5 0 * * *"
    },
    {
      "path": "/api/cron/monitor-blockchain",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/update-prices",
      "schedule": "*/5 * * * *"
    }
  ]
}